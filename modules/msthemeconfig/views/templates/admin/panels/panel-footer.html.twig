    $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE-name').click(function (e) {
      $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE').trigger('click');
    });

    $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE-name').on('dragenter', function (e) {
      e.stopPropagation();
      e.preventDefault();
    });

    $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE-name').on('dragover', function (e) {
      e.stopPropagation();
      e.preventDefault();
    });

    $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE-name').on('drop', function (e) {
      e.preventDefault();
      let files = e.originalEvent.dataTransfer.files;
      $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE')[0].files = files;
      $(this).val(files[0].name);
    });

    $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE').change(function (e) {
      if ($(this)[0].files !== undefined) {
        let files = $(this)[0].files;
        let name = '';

        $.each(files, function (index, value) {
          name += value.name + ', ';
        });

        $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE-name').val(name.slice(0, -2));
      } else // Internet Explorer 9 Compatibility
      {
        let name = $(this).val().split(/[\\/]/);
        $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE-name').val(name[name.length - 1]);
      }
    });

    if (typeof MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE_max_files !== 'undefined') {
      $('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE').closest('form').on('submit', function (e) {
        if ($('#MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE')[0].files.length > MSTHEMECONFIG_FOOTERBOTTOM_FOURTH_IMAGE_max_files) {
          e.preventDefault();
          alert('U kunt maximaal  bestanden uploaden');
        }
      });
    }
  });
</script>
