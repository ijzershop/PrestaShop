        <select name="KOOPMANORDEREXPORT_SELECT_STATUS[]"
                class="w-100 form-control select2-data-input" data-type="order_states" multiple="multiple"
                id="KOOPMANORDEREXPORT_SELECT_STATUS">
          {{ KOOPMANORDEREXPORT_SELECT_STATUS|raw }}
        </select>
        <p class="help-block">
          Choose the status of the orders to export.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select carrier condition
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_CARRIER"
                class="w-100 form-control select2-data-input" data-type="carriers"
                id="KOOPMANORDEREXPORT_SELECT_CARRIER">
          {{ KOOPMANORDEREXPORT_SELECT_CARRIER|raw }}
        </select>
        <p class="help-block">
          Choose the carrier of the orders to export.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4">
        Change order status
      </label>
      <div class="col-xs-8">

        <div class="radio">
          <label><input type="radio" name="KOOPMANORDEREXPORT_UPDATE_BOOL" id="yes"
                        value="1"
                        {% if KOOPMANORDEREXPORT_UPDATE_BOOL == 1 %}checked="checked"{% endif %}>Yes</label>
        </div>
        <div class="radio ">
          <label><input type="radio" name="MSTHEMECONFIG_FIRST_BOX_ACTIVE" id="no" value="0"
                        {% if KOOPMANORDEREXPORT_UPDATE_BOOL == 0 %}checked="checked"{% endif %}>No</label>
        </div>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
