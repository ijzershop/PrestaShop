                id="KOOPMANORDEREXPORT_SELECT_PACKAGELANE_2_PROFILE">
          {{ KOOPMANORDEREXPORT_SELECT_PACKAGELANE_2_PROFILE|raw }}
        </select>
        <p class="help-block">
          Choose the employee profile for usage at the computer at packaging lane 2.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select packing lane 3,
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_PACKAGELANE_3_PROFILE"
                class="w-100 form-control select2-data-input" data-type="profiles"
                id="KOOPMANORDEREXPORT_SELECT_PACKAGELANE_3_PROFILE">
          {{ KOOPMANORDEREXPORT_SELECT_PACKAGELANE_3_PROFILE|raw }}
        </select>
        <p class="help-block">
          Choose the employee profile for usage at the computer at packaging lane 3.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select carrier for pickup order
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_PICKUP_CARRIER"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_SELECT_PICKUP_CARRIER">
          {{ KOOPMANORDEREXPORT_SELECT_PICKUP_CARRIER|raw }}
        </select>
        <p class="help-block">
          Choose the carrier of the orders to pickup.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select status for pickup order
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_PICKUP_STATUS"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_SELECT_PICKUP_STATUS">
          {{ KOOPMANORDEREXPORT_SELECT_PICKUP_STATUS|raw }}
        </select>
        <p class="help-block">
          Choose the status of the orders which are pickup orders..
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select status for order when picked up at store
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_PICKEDUP_STATUS"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_SELECT_PICKEDUP_STATUS">
          {{ KOOPMANORDEREXPORT_SELECT_PICKEDUP_STATUS|raw }}
        </select>
        <p class="help-block">
          Choose the status of the orders which are picked up by customer.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select carrier for added order
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_ADDEDORDER_CARRIER"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_SELECT_ADDEDORDER_CARRIER">
          {{ KOOPMANORDEREXPORT_SELECT_ADDEDORDER_CARRIER|raw }}
        </select>
        <p class="help-block">
          Choose the carrier of the orders to pickup..
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select status for added to order
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_ADDEDORDER_STATUS"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_SELECT_ADDEDORDER_STATUS">
          {{ KOOPMANORDEREXPORT_SELECT_ADDEDORDER_STATUS|raw }}
        </select>
        <p class="help-block">
          Choose the status of the orders which are added to other order.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select status for order when is sent to workshop
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_WORKSHOP_STATUS"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_SELECT_WORKSHOP_STATUS">
          {{ KOOPMANORDEREXPORT_SELECT_WORKSHOP_STATUS|raw }}
        </select>
        <p class="help-block">
          Choose the status of the orders which are send to the workshop..
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select status for order when is sent to waiting for stock
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_WAITING_STOCK_STATUS"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_SELECT_WAITING_STOCK_STATUS">
          {{ KOOPMANORDEREXPORT_SELECT_WAITING_STOCK_STATUS|raw }}
        </select>
        <p class="help-block">
          Choose the status of the orders which are waiting for stock..
        </p>
      </div>
    </div>
    </div>
    <span class="col-xs-12 card-sub-header"><i class="star">*</i> Koopman Retour
        <button class="btn btn-sm btn-link pull-right collapsed" type="button" data-toggle="collapse"
                data-target="#collapseKoopmanRetour" aria-expanded="false" aria-controls="collapseKoopmanRetour">
        <i class="toggle-panel text-white text-decoration-none"></i>
      </button>
      </span>
    <div class="collapse" id="collapseKoopmanRetour">
    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4">
        Show Retour
      </label>
      <div class="col-xs-8">

        <div class="radio">
          <label><input type="radio" name="KOOPMANORDEREXPORT_SHOW_RETOUR" id="yes"
                        value="1"
                        {% if KOOPMANORDEREXPORT_SHOW_RETOUR == 1 %}checked="checked"{% endif %}>Tonen</label>
        </div>
        <div class="radio ">
          <label><input type="radio" name="MSTHEMECONFIG_FIRST_BOX_ACTIVE" id="no" value="0"
                        {% if KOOPMANORDEREXPORT_SHOW_RETOUR == 0 %}checked="checked"{% endif %}>Verbergen</label>
        </div>
      </div>
    </div>


    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        TransMission Status Shipped
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SHIPPED_ACCEPTED_STATUSSES[]"
                class="w-100 form-control select2-data-input" data-type="order_states" multiple="multiple"
                id="KOOPMANORDEREXPORT_SHIPPED_ACCEPTED_STATUSSES">
          {{ KOOPMANORDEREXPORT_SHIPPED_ACCEPTED_STATUSSES|raw }}
        </select>
        <p class="help-block">
          Choose the status for shipping status buttons.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        TransMission Status Retour
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_RETOUR_ACCEPTED_STATUSSES[]"
                class="w-100 form-control select2-data-input" data-type="order_states" multiple="multiple"
                id="KOOPMANORDEREXPORT_RETOUR_ACCEPTED_STATUSSES">
          {{ KOOPMANORDEREXPORT_RETOUR_ACCEPTED_STATUSSES|raw }}
        </select>
        <p class="help-block">
          Choose the status for retour buttons.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        TransMission Status Retour Created
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_RETOUR_CREATED_STATUSSES[]"
                class="w-100 form-control select2-data-input" data-type="order_states" multiple="multiple"
                id="KOOPMANORDEREXPORT_RETOUR_CREATED_STATUSSES">
          {{ KOOPMANORDEREXPORT_RETOUR_CREATED_STATUSSES|raw }}
        </select>
        <p class="help-block">
          Choose the status for when a retour is created.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        TransMission Status Transferred
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_STATUS_TRANSFERRED"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_STATUS_TRANSFERRED">
          {{ KOOPMANORDEREXPORT_STATUS_TRANSFERRED|raw }}
        </select>
        <p class="help-block">
          Choose the status for transferred to Transmission.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        TransMission Status Arrived at depot
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_STATUS_DEPOT"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_STATUS_DEPOT">
          {{ KOOPMANORDEREXPORT_STATUS_DEPOT|raw }}
        </select>
        <p class="help-block">
          Choose the status for arrived at transmission depot.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        TransMission Status Delivery on way
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_STATUS_ON_ROUTE"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_STATUS_ON_ROUTE">
          {{ KOOPMANORDEREXPORT_STATUS_ON_ROUTE|raw }}
        </select>
        <p class="help-block">
          Choose the status for when the delivery person is on the way.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        TransMission Status Delivered
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_STATUS_DELIVERED"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_STATUS_DELIVERED">
          {{ KOOPMANORDEREXPORT_STATUS_DELIVERED|raw }}
        </select>
        <p class="help-block">
          Choose the status for when the package is delivered.
        </p>
      </div>
    </div>

