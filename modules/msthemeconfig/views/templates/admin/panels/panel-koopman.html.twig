      <label class="control-label col-xs-4 required text-right">
        TransMission Status Second try delivery
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_STATUS_ON_ROUTE_SECOND"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_STATUS_ON_ROUTE_SECOND">
          {{ KOOPMANORDEREXPORT_STATUS_ON_ROUTE_SECOND|raw }}
        </select>
        <p class="help-block">
          Choose the status for when the delivery person is on the way for the second delivery try.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        TransMission Status Failed to deliver
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_STATUS_DELIVERY_FAILED"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_STATUS_DELIVERY_FAILED">
          {{ KOOPMANORDEREXPORT_STATUS_DELIVERY_FAILED|raw }}
        </select>
        <p class="help-block">
          Choose the status for when the delivery person failed to deliver the package.
        </p>
      </div>
    </div>
  </div>
  </div>
</div>
