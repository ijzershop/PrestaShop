<div class="card">
  <div class="card-header">
    Koopman Transmission
  </div>
  <div class="card-body">

      <span class="col-xs-12 card-sub-header"><i class="star">*</i> Koopman Algemeen
        <button class="btn btn-sm btn-link pull-right collapsed" type="button" data-toggle="collapse"
                data-target="#collapseKoopmanMain" aria-expanded="false" aria-controls="collapseKoopmanMain">
        <i class="toggle-panel text-white text-decoration-none"></i>
      </button>
      </span>
    <div class="collapse" id="collapseKoopmanMain">
    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select status condition
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_STATUS"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_SELECT_STATUS">
          {{ KOOPMANORDEREXPORT_SELECT_STATUS|raw }}
        </select>
        <p class="help-block">
          Choose the status of the orders to export.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select carrier condition
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_CARRIER"
                class="w-100 form-control select2-data-input" data-type="carriers"
                id="KOOPMANORDEREXPORT_SELECT_CARRIER">
          {{ KOOPMANORDEREXPORT_SELECT_CARRIER|raw }}
        </select>
        <p class="help-block">
          Choose the carrier of the orders to export.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4">
        Change order status
      </label>
      <div class="col-xs-8">

        <div class="radio">
          <label><input type="radio" name="KOOPMANORDEREXPORT_UPDATE_BOOL" id="yes"
                        value="1"
                        {% if KOOPMANORDEREXPORT_UPDATE_BOOL == 1 %}checked="checked"{% endif %}>Yes</label>
        </div>
        <div class="radio ">
          <label><input type="radio" name="MSTHEMECONFIG_FIRST_BOX_ACTIVE" id="no" value="0"
                        {% if KOOPMANORDEREXPORT_UPDATE_BOOL == 0 %}checked="checked"{% endif %}>No</label>
        </div>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Change order-status into
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_UPDATE_STATUS"
                class="w-100 form-control select2-data-input" data-type="order_states"
                id="KOOPMANORDEREXPORT_UPDATE_STATUS">
          KOOPMANORDEREXPORT_UPDATE_STATUS|raw }}
        </select>
        <p class="help-block">
          Choose the status to convert orders into.
        </p>
      </div>
    </div>


    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">SOAP Url</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_SOAP_URL"
               id="KOOPMANORDEREXPORT_SOAP_URL" value="{{ KOOPMANORDEREXPORT_SOAP_URL }}"
               class="form-control">
        <p class="help-block">Enter the koopman api soap url</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">API Username</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_API_USERNAME"
               id="KOOPMANORDEREXPORT_API_USERNAME" value="{{ KOOPMANORDEREXPORT_API_USERNAME }}"
               class="form-control">
        <p class="help-block">Enter the koopman api username</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">API Password</label>
      <div class="col-xs-8">
        <input type="password" name="KOOPMANORDEREXPORT_API_PASSWORD"
               id="KOOPMANORDEREXPORT_API_PASSWORD" value="{{ KOOPMANORDEREXPORT_API_PASSWORD }}"
               class="form-control">
        <p class="help-block">Enter the koopman api password</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Koopman Afzender</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_AFZENDER"
               id="KOOPMANORDEREXPORT_KOOPMAN_AFZENDER" value="{{ KOOPMANORDEREXPORT_KOOPMAN_AFZENDER }}"
               class="form-control">
        <p class="help-block">Enter the koopman afzender</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Koopman Afzender Naam</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERNAAM"
               id="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERNAAM" value="{{ KOOPMANORDEREXPORT_KOOPMAN_AFZENDERNAAM }}"
               class="form-control">
        <p class="help-block">Enter the koopman afzender naam</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Koopman Afzender Sub Naam</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERNAAM2"
               id="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERNAAM2" value="{{ KOOPMANORDEREXPORT_KOOPMAN_AFZENDERNAAM2 }}"
               class="form-control">
        <p class="help-block">Enter the koopman afzender sub naam</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Favicon prefix</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERSTRAAT"
               id="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERSTRAAT" value="{{ KOOPMANORDEREXPORT_KOOPMAN_AFZENDERSTRAAT }}"
               class="form-control">
        <p class="help-block">Enter the koopman afzender straat</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Koopman Afzender Huisnr</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERHUISNR"
               id="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERHUISNR" value="{{ KOOPMANORDEREXPORT_KOOPMAN_AFZENDERHUISNR }}"
               class="form-control">
        <p class="help-block">Enter the koopman afzender huisnummer</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Koopman Afzender Postcode</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERPOSTCODE"
               id="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERPOSTCODE"
               value="{{ KOOPMANORDEREXPORT_KOOPMAN_AFZENDERPOSTCODE }}"
               class="form-control">
        <p class="help-block">Enter the koopman afzender postcode</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Koopman Afzender Plaats</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERPLAATS"
               id="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERPLAATS" value="{{ KOOPMANORDEREXPORT_KOOPMAN_AFZENDERPLAATS }}"
               class="form-control">
        <p class="help-block">Enter the koopman afzender plaats</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Koopman Afzender Land</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERLAND"
               id="KOOPMANORDEREXPORT_KOOPMAN_AFZENDERLAND" value="{{ KOOPMANORDEREXPORT_KOOPMAN_AFZENDERLAND }}"
               class="form-control">
        <p class="help-block">Enter the koopman afzender land</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Koopman Depot</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_DEPOT"
               id="KOOPMANORDEREXPORT_KOOPMAN_DEPOT" value="{{ KOOPMANORDEREXPORT_KOOPMAN_DEPOT }}"
               class="form-control">
        <p class="help-block">Enter the koopman depot</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Koopman Verlader</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_KOOPMAN_VERLADER"
               id="KOOPMANORDEREXPORT_KOOPMAN_VERLADER" value="{{ KOOPMANORDEREXPORT_KOOPMAN_VERLADER }}"
               class="form-control">
        <p class="help-block">Enter the koopman verlader</p>
      </div>
    </div>

    <div class="form-group col-xs-12" data-tab-id="koopman-settings">
      <label class="control-label col-xs-4 text-right">Labels folder (/upload/...)</label>
      <div class="col-xs-8">
        <input type="text" name="KOOPMANORDEREXPORT_LABELS_FOLDER"
               id="KOOPMANORDEREXPORT_LABELS_FOLDER" value="{{ KOOPMANORDEREXPORT_LABELS_FOLDER }}"
               class="form-control">
        <p class="help-block">Enter the label upload folder</p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select packing lane 1
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_PACKAGELANE_1_PROFILE"
                class="w-100 form-control select2-data-input" data-type="profiles"
                id="KOOPMANORDEREXPORT_SELECT_PACKAGELANE_1_PROFILE">
          {{ KOOPMANORDEREXPORT_SELECT_PACKAGELANE_1_PROFILE|raw }}
        </select>
        <p class="help-block">
          Choose the employee profile for usage at the computer at packaging lane 1.
        </p>
      </div>
    </div>

    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select packing lane 2
      </label>
      <div class="col-xs-8">
        <select name="KOOPMANORDEREXPORT_SELECT_PACKAGELANE_2_PROFILE"
