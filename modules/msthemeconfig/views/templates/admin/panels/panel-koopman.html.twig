<div class="card">
  <div class="card-header">
    Koopman Transmission
  </div>
  <div class="card-body">

    {# Testing Domains #}
    <span class="col-xs-12 card-sub-header"><i class="star">*</i> Verzending
        <button class="btn btn-sm btn-link pull-right collapsed" type="button" data-toggle="collapse"
                data-target="#collapseTest" aria-expanded="false" aria-controls="collapseTest">
        <i class="toggle-panel text-white text-decoration-none"></i>
      </button>
      </span>
    <div class="collapse" id="collapseTest">
      <div class="form-group col-xs-12">
        <label class="control-label col-xs-4 text-right">
          Tijd dagafsluiting
        </label>
        <div class="col-xs-8">
          <input type="text" name="MSTHEMECONFIG_SELL_CARRIER_PICKUP_TIME"
                 id="MSTHEMECONFIG_SELL_CARRIER_PICKUP_TIME"
                 value="{{ MSTHEMECONFIG_SELL_CARRIER_PICKUP_TIME }}" class="form-control">
          <p class="help-block">
            Geef de richttijd voor dagafsluiting
          </p>
        </div>
      </div>



        <div class="form-group col-xs-12">
          <label class="control-label col-xs-4 pr-0 required text-right">
            TransMission delivery skip days <button type="button" id="resetCalendar" class="btn btn-xs btn-primary">Reset</button>
          </label>
          <div class="col-xs-8 pl-0">
            <input name="MSTHEMECONFIG_SELL_CARRIER_PICKUP_TIME_SKIPPING_DATES" type="hidden"
                   id="MSTHEMECONFIG_SELL_CARRIER_PICKUP_TIME_SKIPPING_DATES"
                   value="{{ MSTHEMECONFIG_SELL_CARRIER_PICKUP_TIME_SKIPPING_DATES }}">
            <div id="MSTHEMECONFIG_SELL_CARRIER_PICKUP_TIME_SKIPPING_DATES_DIV"></div>
            <p class="help-block">
              Choose the days for when the delivery is not picked up
            </p>
          </div>
        </div>


    </div>




      <span class="col-xs-12 card-sub-header"><i class="star">*</i> Koopman Algemeen
        <button class="btn btn-sm btn-link pull-right collapsed" type="button" data-toggle="collapse"
                data-target="#collapseKoopmanMain" aria-expanded="false" aria-controls="collapseKoopmanMain">
        <i class="toggle-panel text-white text-decoration-none"></i>
      </button>
      </span>
    <div class="collapse" id="collapseKoopmanMain">
    <div class="form-group col-xs-12">
      <label class="control-label col-xs-4 required text-right">
        Select status condition
      </label>
