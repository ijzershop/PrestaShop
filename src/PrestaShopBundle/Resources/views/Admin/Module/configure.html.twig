{% extends 'PrestaShopBundle:Admin:layout.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('themes/default/css/bundle/module/module.css') }}" />
    <link rel="stylesheet" href="{{ asset('themes/default/css/bundle/module/drop.css') }}" />
    <link rel="stylesheet" href="{{ asset('themes/default/css/bundle/module/loader.css') }}" />
    <link rel="stylesheet" href="{{ asset('themes/default/css/bundle/plugins/jquery.pstagger.css') }}" />
    <link rel="stylesheet" href="{{ asset('../js/vendor/node_modules/dropzone/dist/min/dropzone.min.css') }}" />
{% endblock %}

{% block javascripts %}

    {{ parent() }}

    {% javascripts '@PrestaShopBundle/Resources/public/admin/js/module/module.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% javascripts '@PrestaShopBundle/Resources/public/admin/js/plugins/jquery.pstagger.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

{% set requireAddonSearch = false %}

{% block content %}
    {{ moduleContent|raw }}
{% endblock %}
