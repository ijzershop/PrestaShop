
<Directory /var/www/rubentulaseket.nl>
		AllowOverride None
				Require all denied
		</Directory>

<VirtualHost :80>


					DocumentRoot /var/www/clients/client3/web9/web
			
		ServerName rubentulaseket.nl
		ServerAlias www.rubentulaseket.nl
		ServerAdmin webmaster@rubentulaseket.nl


		ErrorLog /var/log/ispconfig/httpd/rubentulaseket.nl/error.log

		Alias /error/ "/var/www/rubentulaseket.nl/web/error/"
		ErrorDocument 400 /error/400.html
		ErrorDocument 401 /error/401.html
		ErrorDocument 403 /error/403.html
		ErrorDocument 404 /error/404.html
		ErrorDocument 405 /error/405.html
		ErrorDocument 500 /error/500.html
		ErrorDocument 502 /error/502.html
		ErrorDocument 503 /error/503.html


		<Directory /var/www/rubentulaseket.nl/web>
				# Clear PHP settings of this website
				<FilesMatch ".+\.ph(p[345]?|t|tml)$">
						SetHandler None
				</FilesMatch>
				Options +SymlinksIfOwnerMatch
				AllowOverride All
								Require all granted
						</Directory>
		<Directory /var/www/clients/client3/web9/web>
				# Clear PHP settings of this website
				<FilesMatch ".+\.ph(p[345]?|t|tml)$">
						SetHandler None
				</FilesMatch>
				Options +SymlinksIfOwnerMatch
				AllowOverride All
								Require all granted
						</Directory>




		# suexec enabled
		<IfModule mod_suexec.c>
			SuexecUserGroup web9 client3
		</IfModule>
		<IfModule mod_fastcgi.c>
				<Directory /var/www/clients/client3/web9/cgi-bin>
										Require all granted
								    </Directory>
				<Directory /var/www/rubentulaseket.nl/web>
					<FilesMatch "\.php[345]?$">
						<If "-f '%{REQUEST_FILENAME}'">
							SetHandler php-fcgi
						</If>
					</FilesMatch>
				</Directory>
				<Directory /var/www/clients/client3/web9/web>
					<FilesMatch "\.php[345]?$">
						<If "-f '%{REQUEST_FILENAME}'">
							SetHandler php-fcgi
						</If>
					</FilesMatch>
				</Directory>
                Action php-fcgi /php-fcgi virtual
				Alias /php-fcgi /var/www/clients/client3/web9/cgi-bin/php-fcgi--80-rubentulaseket.nl
                FastCgiExternalServer /var/www/clients/client3/web9/cgi-bin/php-fcgi--80-rubentulaseket.nl -idle-timeout 300 -socket /var/lib/php5-fpm/web9.sock -pass-header Authorization  -pass-header Content-Type
		</IfModule>
		<IfModule mod_proxy_fcgi.c>
			#ProxyPassMatch ^/(.*\.php[345]?(/.*)?)$ unix:///var/lib/php5-fpm/web9.sock|fcgi://localhost//var/www/clients/client3/web9/web/$1
			<Directory /var/www/rubentulaseket.nl/web>
				<FilesMatch "\.php[345]?$">
					<If "-f '%{REQUEST_FILENAME}'">
						SetHandler "proxy:unix:/var/lib/php5-fpm/web9.sock|fcgi://localhost"
					</If>
				</FilesMatch>
			</Directory>
			<Directory /var/www/clients/client3/web9/web>
				<FilesMatch "\.php[345]?$">
					<If "-f '%{REQUEST_FILENAME}'">
						SetHandler "proxy:unix:/var/lib/php5-fpm/web9.sock|fcgi://localhost"
					</If>
				</FilesMatch>
			</Directory>
			</IfModule>



		# add support for apache mpm_itk
		<IfModule mpm_itk_module>
			AssignUserId web9 client3
		</IfModule>

		<IfModule mod_dav_fs.c>
		# Do not execute PHP files in webdav directory
			<Directory /var/www/clients/client3/web9/webdav>
				<ifModule mod_security2.c>
					SecRuleRemoveById 960015
					SecRuleRemoveById 960032
				</ifModule>
				<FilesMatch "\.ph(p3?|tml)$">
					SetHandler None
				</FilesMatch>
			</Directory>
			DavLockDB /var/www/clients/client3/web9/tmp/DavLock
			# DO NOT REMOVE THE COMMENTS!
			# IF YOU REMOVE THEM, WEBDAV WILL NOT WORK ANYMORE!
      # WEBDAV BEGIN
			# WEBDAV END
		</IfModule>

			
	

</VirtualHost>




